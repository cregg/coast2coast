        }]
    });

    $('#pie-chart').highcharts({
    	title: {
    		text: "Percentage of PPP"
    	},
    	series: [{
    			type: 'pie',
    			data: <%= raw ppp_totals.to_json %>,
    			name: "PPP"
    	}]

    });	
	});
</script>
<div id="wpw-chart"></div>
<div id="tw-chart"></div>
<div id="pie-chart"></div>