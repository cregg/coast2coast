        },
        yAxis:{
        	title: {
        		text: "Power Play Points"	
        	},
        	min: 0
        },
        legend: {
            floating: true,
            backgroundColor: '#FFFFFF',
            align: 'center',
            verticalAlign: 'top',
            y: 40
        },
        series: [{
            name: "At the Helm",
            data: <%= weekly_team_ppp("At the Helm") %>
          	},{ 
            name: "Big Duchene",
            data: <%= weekly_team_ppp("Big Douche-Hayne") %>
            },{ 
            name: "Chris's Team",
            data: <%= weekly_team_ppp("Chris's Team") %>
            },{ 
            name: "Dustin Pennertration",
            data: <%= weekly_team_ppp("Double Pennertration") %>
            },{ 
            name: "Hamhuis Sandwich's",
            data: <%= weekly_team_ppp("Hamhuis Sandwich's") %>
            },{ 
            name: "Keith's Great Team",
            data: <%= weekly_team_ppp("Keith's Great Team") %>
            },{ 
            name: "Lookin Sharp",
            data: <%= weekly_team_ppp("Lookin Sharp") %>
            },{ 
            name: "Teemu's Salami",
            data: <%= weekly_team_ppp("Teemu's Salami") %>
            },{ 
            name: "The Saadfather",
            data: <%= weekly_team_ppp("The Saadfather") %>
            },{
            name: "Yippee Kai Yo Paille",
            data: <%= weekly_team_ppp("Yippee Kai Yo Paille") %>
        }]
    });

		$('#tw-chart').highcharts({
        chart: {
            type: 'line'
        },
        title: {
        		text: "Cumulative PPP"	
        },
        xAxis: {
            categories: ['Week 1', 'Week 2', 'Week 3', 'Week 4', 'Week 5', 'Week 6', 'Week 7', 'Week 8', 'Week 9', 'Week 10', 'Week 11', 'Week 12', 'Week 13']
        },
        yAxis:{
        	title: {
        		text: "PPP",
        	},
        	min: 0
        },
        legend: {
            layout: 'vertical',
            floating: true,
            backgroundColor: '#FFFFFF',
            align: 'left',
            verticalAlign: 'top',
            y: 60,
            x: 50
        },
        series: [{
            name: "At the Helm",
            data: <%= cumulative_team_ppp("At the Helm") %>
          	},{ 
            name: "Big Duchene",
            data: <%= cumulative_team_ppp("Big Douche-Hayne") %>
            },{ 
            name: "Chris's Team",
            data: <%= cumulative_team_ppp("Chris's Team") %>
            },{ 
            name: "Dustin Pennertration",
            data: <%= cumulative_team_ppp("Double Pennertration") %>
            },{ 
            name: "Hamhuis Sandwich's",
            data: <%= cumulative_team_ppp("Hamhuis Sandwich's") %>
            },{ 
            name: "Keith's Great Team",
            data: <%= cumulative_team_ppp("Keith's Great Team") %>
            },{ 
            name: "Lookin Sharp",
            data: <%= cumulative_team_ppp("Lookin Sharp") %>
            },{ 
            name: "Teemu's Salami",
            data: <%= cumulative_team_ppp("Teemu's Salami") %>
            },{ 
