<script type="text/javascript" charset="utf-8">
  $(function () {
     $('#wpw-chart').highcharts({
        chart: {
            type: 'line'
        },
        title: {
        		text: "Plus Minus per Week"	
        },
        xAxis: {
            title:{
                    text: "Weeks"
                },tickInterval: 5
        },
        yAxis:{
        	title: {
        		text: "plus_minus"	
        	}
        },
        legend: {
            floating: true,
            backgroundColor: '#FFFFFF',
            align: 'center',
            verticalAlign: 'top',
            y: 40
        },
        series: [{
            name: "At the Helm",
            data: <%= weekly_team_plus_minus("At the Helm") %>
          	},{ 
            name: "Big Duchene",
            data: <%= weekly_team_plus_minus("Big Douche-Hayne") %>
            },{ 
            name: "Chris's Team",
            data: <%= weekly_team_plus_minus("Chris's Team") %>
            },{ 
            name: "Dustin Pennertration",
            data: <%= weekly_team_plus_minus("Double Pennertration") %>
            },{ 
            name: "Hamhuis Sandwich's",
            data: <%= weekly_team_plus_minus("Hamhuis Sandwich's") %>
            },{ 
            name: "Keith's Great Team",
            data: <%= weekly_team_plus_minus("Keith's Great Team") %>
            },{ 
            name: "Lookin Sharp",
            data: <%= weekly_team_plus_minus("Lookin Sharp") %>
            },{ 
            name: "Teemu's Salami",
            data: <%= weekly_team_plus_minus("Teemu's Salami") %>
            },{ 
            name: "The Saadfather",
            data: <%= weekly_team_plus_minus("The Saadfather") %>
            },{
            name: "Yippee Kai Yo Paille",
            data: <%= weekly_team_plus_minus("Yippee Kai Yo Paille") %>
        }]
    });

		$('#tw-chart').highcharts({
        chart: {
            type: 'line'
        },
        title: {
        		text: "Cumulative Plus-Minus"	
        },
        xAxis: {
            title:{
                    text: "Weeks"
                },tickInterval: 5
        },
        yAxis:{
        	title: {
        		text: "Plus Minus",
        	}
        },
        legend: {
            layout: 'vertical',
            floating: true,
            backgroundColor: '#FFFFFF',
            align: 'left',
            verticalAlign: 'top',
            y: 60,
            x: 50
        },
        series: [{
            name: "At the Helm",
            data: <%= cumulative_team_plus_minus("At the Helm") %>
          	},{ 
            name: "Big Duchene",
            data: <%= cumulative_team_plus_minus("Big Douche-Hayne") %>
            },{ 
            name: "Chris's Team",
            data: <%= cumulative_team_plus_minus("Chris's Team") %>
            },{ 
            name: "Dustin Pennertration",
            data: <%= cumulative_team_plus_minus("Double Pennertration") %>
            },{ 
            name: "Hamhuis Sandwich's",
            data: <%= cumulative_team_plus_minus("Hamhuis Sandwich's") %>
            },{ 
            name: "Keith's Great Team",
            data: <%= cumulative_team_plus_minus("Keith's Great Team") %>
            },{ 
            name: "Lookin Sharp",
            data: <%= cumulative_team_plus_minus("Lookin Sharp") %>
            },{ 
            name: "Teemu's Salami",
            data: <%= cumulative_team_plus_minus("Teemu's Salami") %>
            },{ 
            name: "The Saadfather",
            data: <%= cumulative_team_plus_minus("The Saadfather") %>
            },{
            name: "Yippee Kai Yo Paille",
            data: <%= cumulative_team_plus_minus("Yippee Kai Yo Paille") %>
        }]
    });
});
  $(function () {
    $('#total-chart').highcharts({
    
        chart: {
            type: 'columnrange',
            inverted: false
        },
        
        title: {
            text: 'Highest vs Lowest +/-'
        },
    
        xAxis: {
            categories: ['At the Helm', 'Big Duchence', 'Chris\'s Team', 
            'Dustin Pennertration', 'Hamhuis Sandwich', 'Keith\'s Great Team', 
            'Lookin Sharp', 'Teemu\'s Salami', 'The Saadfather', 
            'Yippee Kai Yo Paille']
        },
        
        yAxis: {
            title: {
                text: 'Plus Minus'
            }
        },
        
        plotOptions: {
            columnrange: {
                dataLabels: {
                    enabled: true,
                    formatter: function () {
                        return this.y;
                    }
                }
            }
        },
    
        series: [{
            name: 'Plus Minus',
            data: <%= high_low_pm %>
        }]
    
    });	
});
</script>
<div id="wpw-chart"></div>
<div id="tw-chart"></div>
<div id="total-chart"></div>